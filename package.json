{
  "name": "avearistov",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "check:build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "size": "size-limit"
  },
  "dependencies": {
    "@astrojs/check": "^0.8.3",
    "@astrojs/cloudflare": "^11.0.1",
    "@astrojs/react": "^3.6.1",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/tailwind": "^5.1.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "astro": "^4.12.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.394.0",
    "react": "^18.3.1",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.4",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@size-limit/file": "^11.1.4",
    "size-limit": "^11.1.4"
  },
  "size-limit": [
    {
      "name": "/*.html",
      "path": "dist/*.html",
      "limit": "25 kB",
      "gzip": true
    },
    {
      "name": "/_astro/*.js",
      "path": "dist/_astro/*.js",
      "limit": "100 kB",
      "gzip": true
    },
    {
      "name": "/_astro/*.css",
      "path": "dist/_astro/*.css",
      "limit": "20 kB",
      "gzip": true
    }
  ]
}
